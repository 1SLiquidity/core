{
  "name": "1sliquidity-local-monitor",
  "version": "1.0.0",
  "description": "Local monitor for 1SLiquidity smart contracts",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "monitor": "ts-node src/index.ts",
    "historical": "ts-node src/index.ts --historical",
    "execute-trades": "ts-node src/execute-trades.ts",
    "test-workflow": "ts-node src/test-workflow.ts",
    "dev": "ts-node --watch src/index.ts",
    "start": "node dist/index.js"
  },
  "keywords": [
    "ethereum",
    "defi",
    "liquidity",
    "monitoring"
  ],
  "author": "",
